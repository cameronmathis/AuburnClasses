<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="openingPane" prefHeight="800.0" prefWidth="1200.0"
            style="-fx-background-color: transparent; -fx-focus-traversable: false;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TabPane fx:id="tabPane" prefHeight="800.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="homeTab" text="Home">
                    <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="770.0" prefWidth="1200.0">
                            <children>
                                <GridPane depthTest="DISABLE" prefHeight="770.0" prefWidth="1200.0"
                                          style="-fx-background-color: #ffffff; -fx-focus-traversable: false;">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308"
                                                           minWidth="-Infinity" prefWidth="350.0"/>
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="75.0"
                                                        vgrow="SOMETIMES"/>
                                        <RowConstraints maxHeight="1.7976931348623157E308" minHeight="-Infinity"
                                                        prefHeight="695.0" vgrow="ALWAYS"/>
                                    </rowConstraints>
                                    <children>
                                        <Text fx:id="HomePageTitle" fill="#57ba86" strokeType="OUTSIDE"
                                              strokeWidth="0.0" text="Home Page" textAlignment="CENTER"
                                              wrappingWidth="373.4362041950226" GridPane.halignment="CENTER"
                                              GridPane.valignment="CENTER">
                                            <GridPane.margin>
                                                <Insets/>
                                            </GridPane.margin>
                                            <font>
                                                <Font name="Arial Bold" size="55.0"/>
                                            </font>
                                        </Text>
                                        <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0"
                                                    vbarPolicy="ALWAYS" GridPane.rowIndex="1">
                                            <content>
                                                <AnchorPane maxWidth="1200.0" minHeight="725.0" minWidth="0.0"
                                                            prefWidth="1200.0" style="-fx-background-color: #ffffff;">
                                                    <children>
                                                        <Text fill="#515151" layoutY="18.0" strokeType="OUTSIDE"
                                                              strokeWidth="0.0" text="Example Post (Work in Progress)">
                                                            <font>
                                                                <Font name="Arial" size="20.0"/>
                                                            </font>
                                                        </Text>
                                                    </children>
                                                </AnchorPane>
                                            </content>
                                        </ScrollPane>
                                        <Line endX="1200.0" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM"/>
                                        <GridPane>
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <MenuButton fx:id="optionsBtn" mnemonicParsing="false" text="Options"
                                                            GridPane.columnIndex="4" GridPane.halignment="CENTER">
                                                    <items>
                                                        <MenuItem fx:id="defineGameBtn" mnemonicParsing="false"
                                                                  text="Define Game"/>
                                                        <MenuItem fx:id="createLeagueBtn" mnemonicParsing="false"
                                                                  text="Create League"/>
                                                        <MenuItem fx:id="changeTypeBtn" mnemonicParsing="false"
                                                                  text="Change Type"/>
                                                        <MenuItem fx:id="createMatchBtn" mnemonicParsing="false"
                                                                  text="Create Match"/>
                                                        <MenuItem fx:id="joinMatchBtn" mnemonicParsing="false"
                                                                  text="Join Match"/>
                                                        <MenuItem fx:id="viewMatchBtn" mnemonicParsing="false"
                                                                  text="View Match"/>
                                                    </items>
                                                </MenuButton>
                                            </children>
                                        </GridPane>
                                    </children>
                                </GridPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="leagueTab" text="League">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TabPane fx:id="tabPane1" prefHeight="800.0" prefWidth="1200.0"
                                         tabClosingPolicy="UNAVAILABLE">
                                    <tabs>
                                        <Tab fx:id="leagueNewsTab" text="News">
                                            <content>
                                                <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
                                                            minHeight="-Infinity" minWidth="-Infinity"
                                                            prefHeight="770.0" prefWidth="1200.0">
                                                    <children>
                                                        <GridPane depthTest="DISABLE" prefHeight="770.0"
                                                                  prefWidth="1200.0"
                                                                  style="-fx-background-color: #ffffff; -fx-focus-traversable: false;">
                                                            <columnConstraints>
                                                                <ColumnConstraints hgrow="ALWAYS"
                                                                                   maxWidth="1.7976931348623157E308"
                                                                                   minWidth="-Infinity"
                                                                                   prefWidth="350.0"/>
                                                            </columnConstraints>
                                                            <rowConstraints>
                                                                <RowConstraints maxHeight="-Infinity"
                                                                                minHeight="-Infinity" prefHeight="75.0"
                                                                                vgrow="SOMETIMES"/>
                                                                <RowConstraints maxHeight="1.7976931348623157E308"
                                                                                minHeight="-Infinity" prefHeight="695.0"
                                                                                vgrow="ALWAYS"/>
                                                            </rowConstraints>
                                                            <children>
                                                                <Text fx:id="HomePageTitle1" fill="#57ba86"
                                                                      strokeType="OUTSIDE" strokeWidth="0.0"
                                                                      text="News Page" textAlignment="CENTER"
                                                                      wrappingWidth="373.4362041950226"
                                                                      GridPane.halignment="CENTER"
                                                                      GridPane.valignment="CENTER">
                                                                    <GridPane.margin>
                                                                        <Insets/>
                                                                    </GridPane.margin>
                                                                    <font>
                                                                        <Font name="Arial Bold" size="55.0"/>
                                                                    </font>
                                                                </Text>
                                                                <ScrollPane hbarPolicy="NEVER" prefHeight="200.0"
                                                                            prefWidth="200.0" vbarPolicy="ALWAYS"
                                                                            GridPane.rowIndex="1">
                                                                    <content>
                                                                        <AnchorPane maxWidth="1200.0" minHeight="725.0"
                                                                                    minWidth="0.0" prefWidth="1200.0"
                                                                                    style="-fx-background-color: #ffffff;">
                                                                            <children>
                                                                                <Text fill="#515151" layoutY="18.0"
                                                                                      strokeType="OUTSIDE"
                                                                                      strokeWidth="0.0"
                                                                                      text="Example Post (Work in Progress)">
                                                                                    <font>
                                                                                        <Font name="Arial" size="20.0"/>
                                                                                    </font>
                                                                                </Text>
                                                                            </children>
                                                                        </AnchorPane>
                                                                    </content>
                                                                </ScrollPane>
                                                                <Line endX="1200.0" GridPane.halignment="CENTER"
                                                                      GridPane.valignment="BOTTOM"/>
                                                            </children>
                                                        </GridPane>
                                                    </children>
                                                </AnchorPane>
                                            </content>
                                        </Tab>
                                        <Tab fx:id="leagueDiscussionTab" text="Discussion">
                                            <content>
                                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                            prefWidth="200.0"/>
                                            </content>
                                        </Tab>
                                        <Tab fx:id="leagueArenaTab" text="Arena">
                                            <content>
                                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                            prefWidth="200.0"/>
                                            </content>
                                        </Tab>
                                    </tabs>
                                </TabPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="followingTab" text="Following">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
                <Tab fx:id="myAdvertisementsTab" text="My Advertisements">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
